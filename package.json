{
  "private": true,
  "scripts": {
    "clean:fable": "dotnet fable clean --yes",
    "fable": "dotnet fable --noCache --sourceMaps --configuration DEBUG",
    "fable:check": "dotnet femto",
    "dev:fable": "dotnet fable watch --noRestore --sourceMaps --configuration DEBUG --runFast parcel serve src/index.html",
    "dev:start": "parcel serve src/index.html",
    "build": "dotnet fable --noCache && parcel build src/index.html",
    "prettier": "prettier --write '**/*.{ts,json,md,yml,js,jsx,mdx}'"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/core": "^2.4.0",
    "@parcel/transformer-image": "^2.4.0",
    "buffer": "^6.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "parcel": "^2.4.0",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-url": "^10.1.3",
    "postcss-custom-properties": "^12.1.5",
    "prettier": "^2.6.0",
    "querystring-es3": "^0.2.1",
    "remotedev": "0.2.9",
    "autoprefixer": "^10.4.2",
    "tailwindcss": "^3.0.23",
    "ts2fable": "^0.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,json,md,yml,js,mdx}": [
      "prettier --write"
    ],
    "src/**/*.fs": [
      "dotnet fantomas ./src/**/*.fs"
    ]
  }
}
